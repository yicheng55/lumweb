<html>
	<head>
		<title>Zeiteinstellung</title>
		<link rel="stylesheet" type="text/css" href="css/design.css" />
		<script type="text/javascript" language="javascript" src="js/funcs_c.js"></script>
	</head>
	<body>
		<div id="container">
			<center><h1>Zeiteinstellung</h1></center>
			<form method="get" action="set.cgi" name="times" id="form-times" onsubmit="return submit_form(this)">
				<center><table style="width: 75%; border:1px solid black;" border="0">
					<tr>
						<td align="center" colspan="4"><b>Tag</b></td>
					</tr>	
					<tr>
						<td align="center"><!--#NumberInputField id=day_hour label=Abs_Stunden max=23 min=0 decimal=0 increment=1 --></td> 
						<td align="center"><!--#NumberInputField id=day_minute label=Abs_Minuten max=59 min=0 decimal=0 increment=10 --></td> 
					</tr>	
					<tr>
						<td align="center" colspan="4"><b>Nacht</b></td>
					</tr>
					<tr>
						<td align="center"><!--#NumberInputField id=night_hour label=Abs_Stunden max=23 min=0 decimal=0 increment=1 --></td> 
						<td align="center"><!--#NumberInputField id=night_minute label=Abs_Minuten max=59 min=0 decimal=0 increment=10 --></td> 
					</tr>	
					<tr>
						<td align="right" colspan="4">
							<!--#CheckboxInputField id=led_ein label=Einschalten -->
							<input type="reset" name="test" value="Zur&uuml;cksetzen" />
							<!--#SubmitInputField label=Speichern form_id=times -->
						</td>
					</tr>
					<tr>
						<td align="right" colspan="4"><!--#Hyperlink label=Menue value= --></td>
					</tr>
				</table>
				<div id="container-times" class="answer">
					<div id="last-update">Letzte Aktualisierung <!--#DateTime--></div>
				</div></center>	
			</form>
		</div>
		<div id="footer">
			<nobr><font style="font-size: 12px">&copy; 2009 - 2010 Anzinger und Hahn in Zusammenarbeit mit <b>Hainzl Industriesysteme</b></font><nobr>
		</div>
	</body>
</html>
